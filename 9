import plotly.graph_objects as go
import numpy as np
x = np.linspace(-5, 5, 100)
y = np.linspace(-5, 5, 100)
X, Y = np.meshgrid(x, y)
Z = np.sin(np.sqrt(X**2 + Y**2))
fig = go.Figure(data=go.Surface(z=Z, x=X, y=Y))
fig.update_layout(scene=dict(
    zaxis_title="Z-axis",
    yaxis_title="Y-axis",
    xaxis_title="X-axis"),
    title="3D Surface Plot")
fig.show()
